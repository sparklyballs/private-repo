<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Bright-game</Name>
<Description>Bright game panel test installation only</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/bright-game/</Registry>
<Repository>sparklyballs/bright-game</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8099</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/bgpanel</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8099]/</WebUI>
<Banner>http://nerdreactor.com/wp-content/uploads/2014/07/League-of-Legends-Banner.png</Banner>
<Icon>http://png-2.findicons.com/files/icons/75/i_like_buttons_3a/512/perspective_button_games.png</Icon>
</Container>

<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>BuildBox</Name>
<Description>Unmark test installation only[br]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/unmark/</Registry>
<Repository>sparklyballs/unmark</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort></HostPort>
<ContainerPort></ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/Build-Scripts</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/deb-out</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI></WebUI>
<Banner></Banner>
<Icon>http://i.imgur.com/9zUSfru.png</Icon>
</Container>

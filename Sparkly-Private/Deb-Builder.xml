<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Deb-Builder</Name>
<Description>Test building .deb for KODI headless</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/test-build-area/</Registry>
<Repository>sparklyballs/test-build-area</Repository>
<BindTime>true</BindTime>
<Privileged>true</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort></HostPort>
<ContainerPort></ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/xbmc</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI></WebUI>
<Banner>http://flashadmin.org/images/linux_banner_243x70.png</Banner>
<Icon>http://www.linuxscrew.com/wp-content/uploads/2008/06/torchlight_tar.png</Icon>
</Container>

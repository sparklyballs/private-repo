<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Calibre</Name>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/sparklyballs/calibre-desktop/</Registry>
  <Repository>sparklyballs/calibre-desktop</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6080</HostPort>
        <ContainerPort>6080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir></ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir></ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:6080]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Container>

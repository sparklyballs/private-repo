<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Headphones</Name>
<Description>Personalised copy of Headphones Docker</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/headphones/</Registry>
<Repository>sparklyballs/headphones</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8181</HostPort>
<ContainerPort>8181</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/XBMC-Media</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/Music</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/Downloads</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://192.168.1.10:8181</WebUI>
<Banner>http://i.imgur.com/u7vqqp5.png</Banner>
<Icon>http://nuxref.files.wordpress.com/2014/07/headphones128x128-icon.png</Icon>
</Container>

<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>BuildBox</Name>
<Overview></Overview>
<Beta>True</Beta>
<Category>Other:</Category>
<Date>2015-08-12</Date>
<Description></Description>
<Registry>https://hub.docker.com/r/linuxserver/docker-kodi-buildbox/</Registry>
<Repository>linuxserver/docker-kodi-buildbox</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort></HostPort>
<ContainerPort></ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name>PUID</Name>
<Value>99</Value>
</Variable>
<Variable>
<Name>PGID</Name>
<Value>100</Value>
</Variable>
<Variable>
<Name>kodiCheckout</Name>
<Value></Value>
</Variable>
<Variable>
<Name>kodi_version</Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI></WebUI>
<Banner></Banner>
<Icon>http://i.imgur.com/9zUSfru.png</Icon>
</Container>

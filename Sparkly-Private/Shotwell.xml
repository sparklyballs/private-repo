<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Shotwell</Name>
  <Description>Shotwell is an image organizer designed to provide personal photo management</Description>
  <Support>http://lime-technology.com/forum/index.php?topic=38232.0</Support>
  <Overview>Shotwell is an image organizer designed to provide personal photo management</Overview>
  <Registry>https://registry.hub.docker.com/u/sparklyballs/shotwell/</Registry>
  <Repository>sparklyballs/shotwell</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/Pictures</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
        <Volume>
      <HostDir></HostDir>
      <ContainerDir>/Pictures-Imported</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/nobody/.config/dconf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/#/client/c/Shotwell</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/SwGg0u1.png</Icon>
</Container>

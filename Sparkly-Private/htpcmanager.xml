<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>HTPC-Manager</Name>
  <Description>
    Personalised copy of htpcmanager Docker</Description>
  <Registry>https://registry.hub.docker.com/u/sparklyballs/htpcmanager/</Registry>
  <Repository>sparklyballs/htpcmanager</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8085</HostPort>
        <ContainerPort>8085</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>EDGE</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8085]/</WebUI>
  <Banner></Banner>
  <Icon>http://www.missingremote.com/legacy/img/stories/eventghost/logo_small.png</Icon>
</Container>

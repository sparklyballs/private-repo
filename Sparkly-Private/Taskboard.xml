<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Taskboard</Name>
<Description>Taskboard test installation only</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/taskboard/</Registry>
<Repository>sparklyballs/taskboard</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8099</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/TaskBoard</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8099]/</WebUI>
<Banner></Banner>
<Icon></Icon>
</Container>

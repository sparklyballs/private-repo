<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Sickbeard</Name>
<Description>Personalised copy of Sickbeard Docker</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/sickbeard/</Registry>
<Repository>sparklyballs/sickbeard</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>8081</HostPort>
<ContainerPort>8081</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name>EDGE</Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/XBMC-Media</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:8081]/</WebUI>
<Banner>http://i.imgur.com/SqhPdyk.png</Banner>
<Icon>http://i.imgur.com/wx5Cp2n.png</Icon>
</Container>

<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Cyca</Name>
<Description>Cyca test installation only[br]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/cyca/</Registry>
<Repository>sparklyballs/cyca</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>80</HostPort>
<ContainerPort>80</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/www/Cyca</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:80]/</WebUI>
<Banner></Banner>
<Icon>http://icons.iconarchive.com/icons/custom-icon-design/pretty-office-7/256/Bookmark-icon.png</Icon>
</Container>

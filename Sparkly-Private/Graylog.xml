<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Graylog2</Name>
<Description>Playing around with something complex as proof of concept</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/sabnzbd/</Registry>
<Repository>sparklyballs/sabnzbd</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>host</Mode>
<Publish>
<Port>
<HostPort>9090</HostPort>
<ContainerPort>9090</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>8080</HostPort>
<ContainerPort>8080</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/Incomplete</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/Downloads</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/mnt/XBMC-Media</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>https://192.168.1.10:9090</WebUI>
<Banner>http://i.imgur.com/5k8nrYA.png</Banner>
<Icon>http://i.imgur.com/GTfJONm.png</Icon>
</Container>

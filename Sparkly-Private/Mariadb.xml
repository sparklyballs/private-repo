<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Mariadb-test</Name>
<Description>
Integrated kodi and mariadb container</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/mariadb/</Registry>
<Repository>sparklyballs/mariadb</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>3308</HostPort>
<ContainerPort>3306</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/db</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir></ContainerDir>
<Mode></Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT]</WebUI>
<Banner></Banner>
<Icon></Icon>
</Container>

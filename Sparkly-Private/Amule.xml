<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Amule</Name>
<Description>Amule test installation only</Description>
<Registry>https://registry.hub.docker.com/u/sparklyballs/amule/</Registry>
<Repository>sparklyballs/amule</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>host</Mode>
<Publish>
<Port>
<HostPort>4665</HostPort>
<ContainerPort>4665</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>4672</HostPort>
<ContainerPort>4672</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>4711</HostPort>
<ContainerPort>4711</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>4712</HostPort>
<ContainerPort>4712</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name></Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/config</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/downloads</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>http://[IP]:[PORT:4711]/</WebUI>
<Banner>http://www.tweaking4all.com/wp-content/uploads/2013/08/logo_emule.png</Banner>
<Icon>http://www.osidb.com/free-icons/png/256x256/apps/amule.png</Icon>
</Container>

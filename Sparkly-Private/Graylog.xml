<?xml version="1.0" encoding="utf-8"?>
<Container>
<Name>Graylog2</Name>
<Description>Graylog2 log analyzer</Description>
<Registry>https://registry.hub.docker.com/u/graylog2/allinone/</Registry>
<Repository>graylog2/allinone</Repository>
<BindTime>true</BindTime>
<Privileged>false</Privileged>
<Networking>
<Mode>bridge</Mode>
<Publish>
<Port>
<HostPort>9000</HostPort>
<ContainerPort>9000</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>12201</HostPort>
<ContainerPort>12201</ContainerPort>
<Protocol>tcp</Protocol>
</Port>
<Port>
<HostPort>514</HostPort>
<ContainerPort>514</ContainerPort>
<Protocol>udp</Protocol>
</Port>
</Publish>
</Networking>
<Environment>
<Variable>
<Name>GRAYLOG2_PASSWORD</Name>
<Value></Value>
</Variable>
<Variable>
<Name>GRAYLOG2_TIMEZONE</Name>
<Value></Value>
</Variable>
</Environment>
<Data>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/log/graylog2</ContainerDir>
<Mode>rw</Mode>
</Volume>
<Volume>
<HostDir></HostDir>
<ContainerDir>/var/opt/graylog2/data</ContainerDir>
<Mode>rw</Mode>
</Volume>
</Data>
<WebUI>https://192.168.1.10:9000</WebUI>
<Banner>http://i.imgur.com/G8zlNUY.png</Banner>
<Icon>http://i.imgur.com/O4mraK7.png</Icon>
</Container>
